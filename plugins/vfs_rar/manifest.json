{
    disable: true,
    source: {
        type: "git",
        url: "https://github.com/shaohao/vfs_rar.git",
        exec: [
            "wget http://www.rarlab.com/rar/unrarsrc-5.5.8.tar.gz && tar zxvf unrarsrc-5.5.8.tar.gz",
            "sed -i 's/#define UNIX_TIME_NS//' unrar/os.hpp"
        ],
        patches: [
            "make.diff"
        ],
    },
    make: {
        type: "make",
        ENV: {
            CXX_LDFLAGS: "-nostdlib -lstdc++ -lc -lpthread"
        },
        root: "",
        out: [
            "vfs_rar.so"
        ],
    }
}
