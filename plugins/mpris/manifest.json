{
    source: {
        type: "git",
        url: "https://github.com/Serranya/deadbeef-mpris2-plugin",
        patches: [
            "make.diff"
        ],
    },
    make: {
        type: "autotools",
        bootstrap: "mkdir -p m4 && autoreconf --install",
        ENV: {
            MPRIS_DEPS_CFLAGS: "$GTK310_CFLAGS",
            MPRIS_DEPS_LIBS: "$GTK310_LIBS",
        },
        root: "",
        out: [
            ".libs/mpris.so",
        ],
    }
}
